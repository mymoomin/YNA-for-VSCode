{
  "name": "yna",
  "displayName": "YNA",
  "description": "YNA for Visual Studio Code",
  "version": "2.0.0",
  "publisher": "mymoomin",
  "license": "MIT",
  "homepage": "https://github.com/mymoomin/YNA-for-VSCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/mymoomin/YNA-for-VSCode"
  },
  "bugs": {
    "url": "https://github.com/mymoomin/YNA-for-VSCode/issues"
  },
  "engines": {
    "vscode": "^0.10.5"
  },
  "keywords": [
    "YNA",
    "Yonni na",
    "42"
  ],
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "update-grammar": "./syntaxes/yna.tmLanguage.json"
  },
  "contributes": {
    "languages": [
      {
        "id": "yna",
        "extensions": [
          ".yna"
        ],
        "aliases": [
          "YNA",
          "Yonni na"
        ],
        "firstLine": "^.*(yna|YNA|oneline)",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yna",
        "scopeName": "source.yna",
        "path": "./syntaxes/yna.tmLanguage.json"
      }
    ]
  }
}
