{
	"information_for_contributors": [
		"This is a basic syntax highlighter for YNA, or Yonni na",
		"If you want to provide a fix or improvement, @me in Kiwi Hangout"
	],
	"version": "0.1.0",
	"name": "YNA",
	"scopeName": "source.yna",
	"patterns": [
		{
			"name": "constant.character.escape.yna",
			"match": "(?<!\\\\)\\\\."
		},
		{
			"name": "meta.diff.header.yna",
			"begin": "{!!",
			"end": "!?}"
		},
		{
			"name": "meta.preprocessor.string.init.yna",
			"begin": "{init}",
			"beginCaptures": {
				"0": {
					"name": "meta.preprocessor.init.yna"
				}
			},
			"end": "{endinit}",
			"endCaptures": {
				"0": {
					"name": "meta.preprocessor.init.yna"
				}
			}
		},
		{
			"name": "comment.block.yna",
			"begin": "{!",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.yna"
				}
			},
			"end": "!}|}(?=;$|$)",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.comment.yna"
				}
			}
		},
		{
			"name": "constant.language.yna",
			"match": "(?<=;)(true|false)(?=;)"
		},
		{
			"name": "storage.yna",
			"match": "(?<={)(set|save|func|member)(?=:)\\b"
		},
		{
			"name": "keyword.control.yna",
			"match": "(?<={)(when|choose|wchoose)(?=:)\\b"
		},
		{
			"name": "support.variable.builtin.yna",
			"match": "(?<={)(tag|me|meid|caller|callerid|channel|channelid|server|serverid|time|uses|newrep|generic|base)(?=[.}])\\b"
		},
		{
			"name": "keyword.yna",
			"match": "(?<={)(arg(\\d|s)|ta(\\d|rgs))(?=})"
		},
		{
			"name": "entity.name.function.yna",
			"match": "(?<={)f_[\\w_]*(?=}|:)|(?<={func:)[\\w_]*(?=;)"
		},
		{
			"name": "meta.definition.variable.name.yna",
			"match": "(?<={set:)[\\w_]*(?=;)|(?<={save:)[\\w_]*(?=;)"
		},
		{
			"name": "variable.yna",
			"match": "((?<={)[\\w_]*(?=[}.])|(?<={set:)[\\w_]*(?=;)|(?<={save:)[\\w_]*(?=;)|(?<={member:)[\\w_]*(?=;))"
		},
		{
			"name": "support.function.builtin.yna", 
			"match": "(?<={)(user|time|rep|num|member|nameof|parse|len|upper|lower|title|oneline|slice|math|void|call)(?=[:;])"
		},
		{
			"name": "invalid.yna",
			"match": "<[a-zA-Z][\\w ]*:[\\w ]+>"
		},
		{
			"name": "keyword.operator.yna",
			"match": "(?<=;)(eq|ne|le|lt|ge|gt|is|in|add|sub|mul|div|idiv|or|and|not|xor|mod|pow|max|min|floor|ceil|round|[+\\-*|<>=]|<=|>=|!=|==|)(?=;)"
		},
		{
			"name": "support.type.yna",
			"match": "(?<=;)(word|letter|number|decimal|error)(?=;)"
		},
		{
			"name": "entity.other.attribute-name.yna",
			"match": "(?<=[.:])(mention|id|display_name|name|guild|roles|joined_at|status|activity|nick|guild_permissions|colou?r|top_role|voice|avatar|avatar_url|bot|created_at|default_avatar|default_avatar_url|discriminator|dm_channel|relationship)(?=[.;}])"
		},
		{
			"name": "constant.numeric.yna",
			"match": "(?<=[\\[;:])\\d+(?=[\\];]|$)"
		},
		{
			"name": "punctuation.separator.yna",
			"match": "[;,]"
		},
		{
			"name": "punctuation.section.braces.begin.yna",
			"match": "{"
		},
		{
			"name": "punctuation.section.braces.end.yna",
			"match": "}"
		}
	]
}